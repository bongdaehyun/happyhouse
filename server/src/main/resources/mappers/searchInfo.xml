<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.happyhouse.repo.DongRepo">
	<select id="selectGugun" resultType="String">
		SELECT distinct gugun FROM dongcode where city=#{city}
	</select>
	<select id="selectDong" resultType="String">
		select dong from dongcode where gugun=#{gugun}
	</select>
	
</mapper>